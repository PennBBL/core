{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {
    "project": {
      "type": "string",
      "description": "Google project id"
    },
    "token": {
      "type": "string",
      "description": "Google Cloud access token"
    },
    "region": {
      "type": "string",
      "description": "Healtcare API region"
    },
    "bqDataset": {
      "type": "string",
      "description": "BigQuery dataset"
    },
    "bqTable": {
      "type": "string",
      "description": "BigQuery table"
    },
    "hcDataset": {
      "type": "string",
      "description": "Healthcare API dataset"
    },
    "hcDicomstore": {
      "type": "string",
      "description": "Healthcare API dicomstore"
    },
    "where": {
      "type": "string",
      "description": "Where clause part of the query"
    },
    "uid": {
      "type": "string",
      "description": "Study or series instance uid"
    },
    "query_id": {
      "type": "string",
      "description": "Id of the query job"
    },
    "study": {
      "type": "boolean",
      "description": "Import the whole study or just the matching series"
    },
    "de_identify": {
      "type": "boolean",
      "description": "De-identify the dicom files"
    },
    "uids": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/uid"
      },
      "description": "List of study/series uids to import"
    },
    "exclude": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/uid"
      },
      "description": "List of study/series uids not to import"
    },
    "query-input": {
      "type": "object",
      "properties": {
        "project": {
          "$ref": "#/definitions/project"
        },
        "dataset": {
          "$ref": "#/definitions/bqDataset"
        },
        "table": {
          "$ref": "#/definitions/bqTable"
        },
        "where": {
          "$ref": "#/definitions/where"
        },
        "token": {
          "$ref": "#/definitions/token"
        }
      },
      "additionalProperties": false
    },
    "details-query-input": {
      "type": "object",
      "properties": {
        "project": {
          "$ref": "#/definitions/project"
        },
        "dataset": {
          "$ref": "#/definitions/bqDataset"
        },
        "table": {
          "$ref": "#/definitions/bqTable"
        },
        "uid": {
          "$ref": "#/definitions/uid"
        },
        "token": {
          "$ref": "#/definitions/token"
        }
      },
      "additionalProperties": false
    },
    "hc-import-input": {
      "type": "object",
      "properties": {
        "query_id": {
          "$ref": "#/definitions/query_id"
        },
        "study": {
          "$ref": "#/definitions/study"
        },
        "uids": {
          "$ref": "#/definitions/uids"
        },
        "exclude": {
          "$ref": "#/definitions/exclude"
        },
        "group_id": {
          "$ref": "common.json#/definitions/lowercase-string-id"
        },
        "project_label": {
          "$ref": "common.json#/definitions/label"
        },
        "project": {
          "$ref": "#/definitions/project"
        },
        "location": {
          "$ref": "#/definitions/region"
        },
        "dataset": {
          "$ref": "#/definitions/hcDataset"
        },
        "dicomstore": {
          "$ref": "#/definitions/hcDicomstore"
        },
        "de_identify": {
          "$ref": "#/definitions/de_identify"
        },
        "token": {
          "$ref": "#/definitions/token"
        }
      },
      "additionalProperties": false
    },
    "get-schema-input": {
      "type": "object",
      "properties": {
        "project": {
          "$ref": "#/definitions/project"
        },
        "dataset": {
          "$ref": "#/definitions/bqDataset"
        },
        "table": {
          "$ref": "#/definitions/bqTable"
        },
        "token": {
          "$ref": "#/definitions/token"
        }
      },
      "additionalProperties": false
    }
  }
}